  <%- include('partials/header'); %>
  <div class="container">

    <nav class="navbar navbar-expand-lg navbar-light">
      <a class="navbar-brand main-prefix" href="#"><%=brand%></a>
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarSupportedContent">
        <ul class="navbar-nav ml-auto">
          <% navMenu.forEach((menu)=>{ %>
          <li class="navitem<%if(menu['active']){%> active<%}%>">
          <a class="nav-link" href="<%=menu['link']%>"><%=menu['name']%></a>
          </li>
          <% }) %>
        </ul>
      </div>
    </nav>

    <div class="row mt-4">
      <% content.forEach(function(guide){%>
      <div class="col-lg-3 col-md-4 col-sm-6">
        <div class="item mb-4 well ">
          <div class="item-res">
            <a href="<%=guide['url']%>"><img src="<%=guide['img']%>" alt="<%=guide['alt']%>"></a>
          </div>
          <a href="<%=guide['url']%>" class="item-name"><%=guide['name']%></a>
        </div>
      </div>
      <% }) %>
    </div>

<%- include('partials/footer'); %>
